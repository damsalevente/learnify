<!DOCTYPE html>
<html>
  <head>
   
    <% include ../partials/head %>
  </head>
  <header>
    <% include ../partials/header %>
  </header>
  <body class="grey darken-4 green-text text-lighten-1">
       
            
       
     
          <div class="row">
            <div class="col s3 m3">
              <h4 class=flow-text>Sidenavbar</h4>
             <div class="collection">
          
              <a href="#!" class="collection-item grey darken-4 green-text text-lighten-1">Alvin</a>
              <a href="#!" class="collection-item grey darken-4 green-text text-lighten-1">Alvin</a>
              <a href="#!" class="collection-item grey darken-4 green-text text-lighten-1">Alvin</a>
                <a href="#!" class="collection-item grey darken-4 green-text text-lighten-1">Alvin</a>
              </div>
                    
            </div>
            <div class="col s6 m6">
                <h4 class=flow-text>                List of courses
                  </h4>
                      <ul class="collection">
                        <% course.forEach(course =>{ %>
                          <li class="collection-item avatar grey darken-4 green-text text-lighten-1">
                            <% if (course.finished){ %>
                                <i class="material-icons circle grey darken-4 green-text text-lighten-1">check</i>

                                <% } else{ %>
                           
                                <i class="material-icons circle grey darken-4 green-text text-lighten-1">clear</i>

                            <% } %>
                            <span class="title grey darken-4 green-text text-lighten-1"> <%= course.name  %></span>
                            <p> <%= course.time %><br>
                               
                            </p>
                            <a href="#!" class="secondary-content "><i class="material-icons">grade</i></a>
                          </li>
                         <% }); %>
                        </ul>
            </div>                                                  
                    <div class="col s3 m3">
                        <h4 class=flow-text>               Chatbox
                          </h4>
                      
                    </div>
                
                 
                
             
              
           
               
                   

           </div>
          </div>
         
    <% include ../partials/foot %>
  </body>
</html>
